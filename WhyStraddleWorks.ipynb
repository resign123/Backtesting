   "source": [
    "# Calculate the percentage of daily moves that are greater than 0\n",
    "positive_moves = (daily_moves['daily_move'] < 0).sum()\n",
    "total_moves = len(daily_moves)\n",
    "\n",
    "percentage_positive_moves = (positive_moves / total_moves) * 100\n",
    "\n",
    "# Print result\n",
    "print(f\"Percentage of vix crash: {percentage_positive_moves:.2f}%\")\n"
