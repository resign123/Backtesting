   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import plotly.graph_objects as go\n",
    "\n",
    "# --- Define Bin Ranges ---\n",
    "daily_bins = np.arange(-3, 3.2, 0.2)  # From -3% to 3% in 0.2% steps\n",
    "weekly_bins = np.arange(-5, 5.2, 0.5)  # From -5% to 5% \n",
    "\n",
    "\n",
    "daily_moves = df[['date', 'daily_move']]\n",
    "weekly_moves = df[['date', 'weekly_move']]\n",
    "\n",
    "\n",
    "\n",
    "# --- Extract Numeric Data ---\n",
    "daily_move_values = daily_moves['daily_move'].values\n",
    "weekly_move_values = weekly_moves['weekly_move'].dropna().values\n",
    "\n",
    "# --- 1. Percentage of Positive Moves ---\n",
    "daily_positive_pct = (daily_move_values > 0).mean() * 100\n",
    "weekly_positive_pct = (weekly_move_values > 0).mean() * 100\n",
    "\n",
    "print(f\"ðŸ“ˆ Percentage of Positive Daily Moves: {daily_positive_pct:.2f}%\")\n",
    "print(f\"ðŸ“Š Percentage of Positive Weekly Moves: {weekly_positive_pct:.2f}%\")\n",
    "\n",
    "# --- 2. Frequency Distribution ---\n",
    "daily_hist, daily_bin_edges = np.histogram(daily_move_values, bins=daily_bins, density=True)\n",
    "weekly_hist, weekly_bin_edges = np.histogram(weekly_move_values, bins=weekly_bins, density=True)\n",
    "\n",
    "# Convert to percentage format\n",
    "daily_hist_pct = daily_hist * 100 * np.diff(daily_bin_edges)\n",
    "weekly_hist_pct = weekly_hist * 100 * np.diff(weekly_bin_edges)\n",
    "\n",
    "# --- 3. Tick-wise Distribution ---\n",
    "print(\"\\nðŸ“Š Tick-wise Distribution of Daily Moves (% occurrence in each bin):\")\n",
    "daily_distribution = []\n",
    "for i in range(len(daily_hist_pct)):\n",
    "    daily_distribution.append(f\"{daily_bin_edges[i]:.1f}% to {daily_bin_edges[i+1]:.1f}%: {daily_hist_pct[i]:.2f}%\")\n",
    "    print(daily_distribution[-1])\n",
    "\n",
    "print(\"\\nðŸ“Š Tick-wise Distribution of Weekly Moves (% occurrence in each bin):\")\n",
    "weekly_distribution = []\n",
    "for i in range(len(weekly_hist_pct)):\n",
    "    weekly_distribution.append(f\"{weekly_bin_edges[i]:.1f}% to {weekly_bin_edges[i+1]:.1f}%: {weekly_hist_pct[i]:.2f}%\")\n",
    "    print(weekly_distribution[-1])\n",
    "\n",
    "# --- 5. Longest Streaks of Gains/Losses ---\n",
    "def longest_streak(series):\n",
    "    max_streak, current_streak = 0, 0\n",
    "    prev_sign = None\n",
    "    \n",
    "    for val in series:\n",
    "        sign = val > 0  # True if positive, False if negative\n",
    "        if sign == prev_sign:\n",
    "            current_streak += 1\n",
    "        else:\n",
    "            max_streak = max(max_streak, current_streak)\n",
    "            current_streak = 1\n",
    "        prev_sign = sign\n",
    "    \n",
    "    return max(max_streak, current_streak)\n",
    "\n",
    "longest_daily_gain_streak = longest_streak(daily_move_values)\n",
    "longest_daily_loss_streak = longest_streak(-daily_move_values)\n",
    "\n",
    "longest_weekly_gain_streak = longest_streak(weekly_move_values)\n",
    "longest_weekly_loss_streak = longest_streak(-weekly_move_values)\n",
    "\n",
    "print(f\"\\nðŸ”¥ Longest Daily Gain Streak: {longest_daily_gain_streak} days\")\n",
    "print(f\"ðŸ”» Longest Daily Loss Streak: {longest_daily_loss_streak} days\")\n",
    "\n",
    "print(f\"\\nðŸ”¥ Longest Weekly Gain Streak: {longest_weekly_gain_streak} weeks\")\n",
    "print(f\"ðŸ”» Longest Weekly Loss Streak: {longest_weekly_loss_streak} weeks\")\n",
    "\n",
    "# --- 6. Interactive Histogram (Scrollable) ---\n",
    "fig = go.Figure()\n",
