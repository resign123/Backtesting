    "import pandas as pd\n",
    "\n",
    "def parse_mixed_datetime(date_str):\n",
    "    \"\"\"Try parsing datetime with and without seconds\"\"\"\n",
    "    try:\n",
    "        return pd.to_datetime(date_str, format='%d-%m-%Y %H:%M:%S')  # First try with seconds\n",
    "    except ValueError:\n",
    "        return pd.to_datetime(date_str, format='%d-%m-%Y %H:%M')  # If fails, try without seconds\n",
    "\n",
    "# Apply function to each row in 'datetime' column\n",
    "df['datetime'] = df['datetime'].apply(parse_mixed_datetime)\n",
    "\n",
    "# Ensure datetime column has a uniform format\n",
    "df['datetime'] = df['datetime'].dt.strftime('%d-%m-%Y %H:%M:%S')  \n",
    "df['datetime'] = pd.to_datetime(df['datetime'], format='%d-%m-%Y %H:%M:%S')\n",
    "\n",
    "# Extract date, time, and weekday\n",
    "df['date'] = df['datetime'].dt.date\n",
    "df['time'] = df['datetime'].dt.time\n",
    "df['weekday'] = df['datetime'].dt.weekday  # Monday=0, Thursday=3\n",
    "\n",
    "# --- Handle Duplicate Entries ---\n",
    "df = df.groupby(['date', 'time']).agg({'close': 'mean'}).reset_index()\n",
    "\n",
    "# --- Daily Move Calculation ---\n",
    "daily_prices = df[(df['time'] == pd.to_datetime('09:20:00').time()) | \n",
    "                  (df['time'] == pd.to_datetime('15:20:00').time())]\n",
    "\n",
    "# Ensure we have only one price per date-time before pivoting\n",
    "daily_pivot = daily_prices.pivot(index='date', columns='time', values='close').dropna()\n",
    "\n",
    "# Rename columns properly\n",
    "daily_pivot.columns = ['price_9_20', 'price_3_20']\n",
    "\n",
    "# Calculate daily move\n",
    "daily_pivot['daily_move'] = ((daily_pivot['price_3_20'] - daily_pivot['price_9_20']) /\n",
    "                             daily_pivot['price_9_20']) * 100\n",
    "\n",
    "# Extract final values\n",
    "daily_moves = daily_pivot[['daily_move']].reset_index()\n",
    "\n",
    "# Print results\n",
    "print(\"Daily Moves:\")\n",
    "print(daily_moves)\n"
   ]
